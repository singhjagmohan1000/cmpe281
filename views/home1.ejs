    <section id="analytics">
    <div class="container">
        <div class="row">

            <div class="sec-title text-center wow animated fadeInDown">
                <h2>Real time analytics</h2>
            </div>

            <link href="https://www.jqueryscript.net/css/jquerysctipttop.css" rel="stylesheet" type="text/css">
            <link href="stylesheets/monthpicker.css" rel="stylesheet" type="text/css">
            <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/jquery-ui.min.js"></script>
            <link href="https://www.jqueryscript.net/css/jquerysctipttop.css" rel="stylesheet" type="text/css">

            <link rel="stylesheet" type="text/css" media="screen" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/themes/base/jquery-ui.css">
            <p>Choose the month you wish to view the logs for:</p> <input id="demo-1" type="text" />
        </div>

        <script src="js/monthpicker.js"></script>
        <div id="chartContainer" style="height: 300px; width: 100%;"></div>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/canvasjs/1.7.0/jquery.canvasjs.min.js"></script>
        <script>
            var monthselected ="";
            $('#demo-1').Monthpicker({
                dateFormat: 'yymmdd',
                onSelect: function (dateText, inst) {
                    var dateStringForApi = $('#demo-1').val();
                    if (dateStringForApi == "02/2016") {
                        monthselected = "february";
                    }
                    if (dateStringForApi == "03/2016") {
                        monthselected = "march";
                    }
                    if (dateStringForApi == "01/2016") {
                        monthselected = "january";
                    }
                    if (dateStringForApi == "04/2016") {
